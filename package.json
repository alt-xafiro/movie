{
  "private": true,
  "license": "WTFPL",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:https": "next dev --turbopack --experimental-https",
    "build": "next build",
    "start": "next start",
    "start:build": "next build && next start",
    "lint": "pnpm lint:eslint && pnpm lint:ts && pnpm lint:fsd",
    "lint:eslint": "next lint",
    "lint:ts": "tsc --noEmit",
    "lint:fsd": "steiger ./src",
    "format": "prettier --write --ignore-unknown .",
    "format:check": "prettier --check --ignore-unknown .",
    "test": "pnpm unit:no-watch && pnpm e2e",
    "unit": "vitest",
    "unit:no-watch": "vitest --watch=false",
    "e2e": "pnpm build && pnpm e2e:test-only",
    "e2e:test-only": "pnpm exec playwright test",
    "e2e:show-report": "pnpm exec playwright show-report",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky"
  },
  "browserslist": [
    ">0.3%",
    "last 4 versions",
    "not dead",
    "Safari >= 15",
    "iOS >= 15",
    "Firefox ESR",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": "22",
    "pnpm": "10"
  },
  "packageManager": "pnpm@10.7.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  },
  "dependencies": {
    "clsx": "2.1.1",
    "next": "15.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.34.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@commitlint/types": "19.8.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.24.0",
    "@playwright/test": "1.51.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "22.13.14",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "autoprefixer": "10.4.21",
    "eslint": "9.24.0",
    "eslint-config-next": "15.3.0",
    "eslint-config-prettier": "10.1.1",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "steiger": "0.5.6",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.1"
  }
}
